/**
 * Contains most of the cJSON implementation.
 **/
#ifndef JSON_C
#define JSON_C
#include "remote/cJSON/cJSON.h"
#include "remote/cJSON/cJSON.c"
#include <stdlib.h>
#include <string.h>
#include <stdio.h>

#define json_is_array(json) \
	cJSON_IsArray(json)


static cJSON *
json_get_path (/*cJSON *json,*/ char *path)
{if (!path) return NULL;

	cJSON *out;

	// tok size
	const int path_s = strlen (path);
	char *tok = (char *)malloc (path_s);

	// tok placement
	int tok_p = 0;
	for (int i = 0; i < path_s; i++)
	  {
		if (path[i] == '.')
		  {
			tok_p = i+1;
			tok[tok_p] = '\0';

			// tok is now valid
		 	printf ("%s\n", tok);	
		  }
		tok[i - tok_p] = path[i];		
	  }
	return out;
}

#endif

int main (void)
{
	json_get_path("test.1.2.3");
	return 0;
}
